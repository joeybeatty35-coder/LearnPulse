{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ]
}
{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    { "src": "^/blog$", "dest": "/blog/index.html" },
    { "src": "^/blog/(.*)$", "dest": "/blog/$1" },
    { "src": "^/$", "dest": "/index.html" }
  ]
}